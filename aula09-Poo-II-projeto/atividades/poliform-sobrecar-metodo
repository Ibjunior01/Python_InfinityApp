class Calculadora:
    def somar(self, a, b):
        if isinstance(a, str) and isinstance(b, str):
            return a + b
        elif isinstance(a, int) and isinstance(b, int):
            return a + b
        else:
            return "Tipos incompatíveis"

# Exemplos
calc = Calculadora()
print(calc.somar(5, 3))         # 8
print(calc.somar("Olá, ", "Mundo!"))  # "Olá, Mundo!"